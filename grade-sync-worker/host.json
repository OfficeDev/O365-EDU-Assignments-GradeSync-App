{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            }
        }
    },
    "extensions": {
        "queues": {
            "maxPollingInterval": "00:00:02",
            "batchSize": 12,
            "maxDequeueCount": 3,
            "newBatchThreshold": 5
        }
    }
}

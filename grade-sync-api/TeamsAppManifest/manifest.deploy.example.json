{
    "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.12/MicrosoftTeams.schema.json",
    "manifestVersion": "1.12",
    "id": "<create-a-guid>",
    "version": "1.0.0",
    "packageName": "com.custom.tab",
    "developer": {
      "name": "<Your district/org name>",
      "websiteUrl": "https://<your-app-service-url>.azurewebsites.net/",
      "privacyUrl": "https://<your-app-service-url>.azurewebsites.net/privacy",
      "termsOfUseUrl": "https://<your-app-service-url>.azurewebsites.net/tou",
      "mpnId": ""
    },
    "name": {
      "short": "YourAppName",
      "full": "personalTab"
    },
    "description": {
      "short": "short description here",
      "full": "full description here"
    },
    "icons": {
      "outline": "outline.png",
      "color": "color.png"
    },
    "accentColor": "#D85028",
    "configurableTabs": [],
    "staticTabs": [
      {
        "entityId": "index",
        "name": "Grade Sync",
        "contentUrl": "https://<your-app-service-url>.azurewebsites.net/",
        "websiteUrl": "https://<your-app-service-url>.azurewebsites.net/",
        "scopes": [
          "team"
        ]
      }
    ],
    "bots": [],
    "connectors": [],
    "composeExtensions": [],
    "permissions": [
      "identity",
      "messageTeamMembers"
    ],
    "validDomains": [
      "*.azurewebsites.net"
    ],
    "webApplicationInfo": {
      "id": "<Application (client) ID>",
      "resource": "<Application ID URI>"
    }
  }
  